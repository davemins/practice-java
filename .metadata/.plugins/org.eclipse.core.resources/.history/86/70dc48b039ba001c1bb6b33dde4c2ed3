package practice;

public class Person {
	
	private double height;
	private double weight;
	
	public double getHeight() {
		return height;
	}
	public void setHeight(double height) {
		this.height = height;
	}
	public double getWeight() {
		return weight;
	}
	public void setWeight(double weight) {
		this.weight = weight;
	}
	
	public Person(double height, double weight) {
		super();
		this.height = height;
		this.weight = weight;
	}
	
}
