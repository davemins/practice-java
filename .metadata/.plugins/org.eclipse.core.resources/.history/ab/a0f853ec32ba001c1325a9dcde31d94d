package practice;

public class Node {
	
	private int height;
	private int weight;
	
	public int getHeight() {
		return height;
	}
	public void setHeight(int height) {
		this.height = height;
	}
	public int getWeight() {
		return weight;
	}
	public void setWeight(int weight) {
		this.weight = weight;
	}
	
	public Node(int height, int weight) {
		super();
		this.height = height;
		this.weight = weight;
	}
	
	public void show() {
		System.out.println("Å° Ã¼Áß : " + getHeight() + getWeight());
	}
}
